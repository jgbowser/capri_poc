{
  "name": "capri_poc",
  "version": "1.0.0",
  "description": "POC for LangChain based chatbot app for zoning ordinances in various cities/towns/municipalities",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iinteractive/capri_poc.git"
  },
  "author": "John Bowser",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": "20.5.0",
    "npm": "9.8.0"
  },
  "scripts": {
    "docker:install": "./scripts/docker-install.sh",
    "docker:psql": "PGOPTIONS='--search_path=capri-poc,public' PGPASSWORD='docker' psql -h localhost -U postgres -d capri-poc",
    "docker:start": "docker start capri-poc-docker",
    "docker:stop": "docker stop capri-poc-docker",
    "start": "tsc && node build/index.js"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "langchain": "^0.0.189",
    "openai": "^4.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
